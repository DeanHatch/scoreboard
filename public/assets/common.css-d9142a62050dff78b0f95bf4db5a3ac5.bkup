     /* Global styling */


   html, body {height: 100%;
               padding: 0;
               margin: 0;
           text-align: center;}

   body { background-color: black; }

  label { font: normal 8pt Verdana, Arial, Helvetica, sans-serif; }

   p, h1, h2, h3, h4, h5, h6 {
             display: block;
       margin-bottom: 0;
          margin-top: 0;
      padding-bottom: 5px;
         padding-top: 5px;
        border-style: none;}

   p, dt, dd, td, th {
            font: normal 11px/140% Verdana, Arial, Helvetica, sans-serif;
           color: #666;}

     /* ??? */
   .current { font-weight: bold; }


     /* All links in RED */

   a { color: red; }


     /* Headings differ from each other only in font size */

   h1, h2, h3, h4, h5, h6 { text-align: center;
                                 color: black;}
   h1 {   font: bold 14px/140% Verdana, Arial, Helvetica, sans-serif;}

   h2 {   font: bold 12px/140% Verdana, Arial, Helvetica, sans-serif;}

   h3 {   font: bold 11px/140% Verdana, Arial, Helvetica, sans-serif;}

   h4 {   font: bold 10px/140% Verdana, Arial, Helvetica, sans-serif;}

   h5 {   font: bold 9px/140% Verdana, Arial, Helvetica, sans-serif;}

   h6 {   font: bold 8px/140% Verdana, Arial, Helvetica, sans-serif;}




     /*  */
   div.container { width: 940px;
                  margin: 0 auto;
                position: relative;
                 z-index: 2;}

   div.loginContent { position: relative;
                       top: 3;
                  margin: 3 0;width: 25px;
             background-color: orange;
                 z-index: 4;}



     /*  */

   div.mainContent { width: 520px;
                       height: 100%;
                       top: 0;
                  margin: 0 auto;
                 float: right;
               padding: 160px 100px 50px 100px;
             background-color: white;

                 z-index: 2;}


   div.dividedContent { width: 100%;
                       top: 0;
                  margin: 0 auto;
             background-color: white;

                 z-index: 3;}

   div.leftContent { position: relative;
                 width: 49%;
                       top: 0;
                  margin: 0 auto;
                 float: right;
             background-color: white;

                 z-index: 4;}

   div.rightContent { position: relative;
                 width: 49%;
                       top: 0;
                  margin: 0 auto;
                 float: left;
             background-color: white;
                 z-index: 4;}


     /*  */

   div.header {	width: 940px;

               height: 120px;
                  top: 20px;
               margin: 0px 0 20px 0;
              padding: 20px 0 0 0;
             position: absolute;
              z-index: 4;}
   div.inner-header {
	width: 940px;
                       height: 120px;
             background-color: #666666;}
   div.inner-header img { width: 940px;
                         height: 120px;
               background-color: #666666;}
   div.inner-header strong {
 left: 2%;
                
    top: 72%;

                 display: block;
                     font:  normal 24px/120% Helvetica, Arial, sans-serif;

                  color: #fcf302;
                 position: absolute;
                   margin: 0;
                  padding: 0;
                  z-index: 3;
}



   div.wrapper {padding: 5px;}
   p.wrapper {padding: 5px;
                color: green;
           text-align: center;
            font-size: 8pt;}

   div.widebody {overflow: auto;}

   div.notTooTall {overflow: auto;
                     height: 520px;}

   div.prettyShort {overflow: auto;
                      height: 300px;}

   div.footer { width: 520px;
                  margin: 0 auto;
                 float: right;
                color: #777;
                color: yellow;
            font: normal 11px/140% Verdana, Arial, Helvetica, sans-serif;
          font-size: 10pt;
               padding: 10px 100px 20px 100px;}

   div.sideMatter { width: 240px;
                   position: absolute;
                       top: 160px;
                    border: solid;
              border-width: 1px;
              border-color: red;
                   display: block; }

   a.nav { color: black;
           text-decoration: none; }
   a.nav:hover { font-weight: bold; }
   li.nav { border-bottom: dotted white 1px;
            text-align: left;
             font: normal 11px/140% Verdana, Arial, Helvetica, sans-serif;
            color: black; }
   li.nav1 { border-bottom: dotted white 1px;
             font: normal 11px/140% Verdana, Arial, Helvetica, sans-serif;
            color: black; }
   ul.nav1 { margin: 0;
            padding: 0;
         list-style: none; }
   li.level1 { padding: 2px 1em;
             font: normal 11px/140% Verdana, Arial, Helvetica, sans-serif;}
   li.level2 { padding: 2px 1em 2px 2em;
             font: normal 11px/140% Verdana, Arial, Helvetica, sans-serif;}
   li.level3 { padding: 2px 1em 2px 3em;
             font: normal 11px/140% Verdana, Arial, Helvetica, sans-serif;}
   li.level4 { padding: 2px 1em 2px 4em;
             font: normal 11px/140% Verdana, Arial, Helvetica, sans-serif;}
   li.level5 { padding: 2px 1em 2px 5em;
             font: normal 11px/140% Verdana, Arial, Helvetica, sans-serif;}
   div.navPanel {    width: 240px;
                       top: 200px;
                   padding: 0 0 50px;
                   display: block;
                background: #6F7F7F;}
   div.horizontal {  width: 520px;
                   display: block;
                background: #8F9F9F;}
   li.horizontal { border-right: dotted white 1px;
                    border-left: dotted white 1px;
                        display: inline;
                  padding-right: 1em;
                   padding-left: 1em;
                           font: normal 11px/140% Verdana, Arial, Helvetica, sans-serif;
                          color: black; }

   div.instructions { width: 240px;
                    padding: 0 0 5px;
                    display: block;
                       font: normal 9pt Verdana, Arial, Helvetica;
                      color: yellow;
                 background: #6F7F7F;}


table.schedule {text-align: center;
