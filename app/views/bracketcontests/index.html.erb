<h1>Bracket Contests</h1>
<h2>for <%= @bracket.fullname %></h2>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bracketcontests.each do |bracketcontest| %>
      <tr>
      <%= render partial: 'editable_bracket_contest' , locals: { bracketcontest: bracketcontest }  %>
      <br/>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<%= link_to 'New Bracketcontest', new_competition_bracket_bracketcontest_path %>
<br/>
<%= link_to 'Back to Bracket Home', competition_brackets_path %>
