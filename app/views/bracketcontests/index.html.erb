<h1>Bracket Contest Listing</h1>


<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Venue</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bracketcontests.each do |bracketcontest| %>
      <tr>
        <td><%= bracketcontest.display_date %></td>
        <td><%= bracketcontest.display_time %></td>
        <td><%= bracketcontest.venue_name %></td>
        <td><%= bracketcontest.status %></td>
        <td><%= link_to 'Show', bracketcontest %></td>
        <td><%= link_to 'Edit', edit_bracketcontest_path(bracketcontest) %></td>
        <td><%= link_to 'Destroy', bracketcontest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bracketcontest', new_bracketcontest_path , id: "bazooka"%>

<%= link_to 'New Regularcontest', new_regularcontest_path , id: "handgrenade"%>
