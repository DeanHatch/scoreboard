<h1>Dump of Bracket Contests</h1>

<br>

<table>
  <thead>
    <tr>
        <th>Date</th>
        <th>Time</th>
        <th>Teams</th>
        <th>Contestant IDs</th>
        <th>Where</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  
    <% @bracketcontests.sort{ |a,b| a.updated_at <=> b.updated_at }.reverse.each do |bracketcontest| %>
      <tr>
        <td><%= bracketcontest.display_date  %></td>
        <td><%= bracketcontest.display_time  %></td>
        <td><%= bracketcontest.awaycontestant.fullname if bracketcontest.awaycontestant %>
        at <%= bracketcontest.awaycontestant.opponent.fullname if bracketcontest.awaycontestant %></td>
        <td><%= bracketcontest.awaycontestant.id if bracketcontest.awaycontestant %>
        -- <%= bracketcontest.homecontestant.id if bracketcontest.homecontestant %></td>
        <td><%= bracketcontest.venue_name  %></td>
        <td><%= link_to 'Show', competition_bracketcontest_path(@competition, bracketcontest)  %></td>
        <td><%= link_to 'Edit', edit_competition_bracketcontest_path(@competition, bracketcontest) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New bracketcontest', new_competition_bracketcontest_path %>
