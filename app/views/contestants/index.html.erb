<h1>Listing contestants</h1>

<table>
  <thead>
    <tr>
      <th>Contest</th>
      <th>Homeaway</th>
      <th>Team</th>
      <th>Score</th>
      <th>Forfeit</th>
      <th>Contestanttype</th>
      <th>Seeding</th>
      <th>Bracketcontest</th>
      <th>Grouping</th>
      <th>Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contestants.each do |contestant| %>
      <tr>
        <td><%= contestant.contest %></td>
        <td><%= contestant.homeaway %></td>
        <td><%= contestant.team %></td>
        <td><%= contestant.score %></td>
        <td><%= contestant.forfeit %></td>
        <td><%= contestant.contestanttype %></td>
        <td><%= contestant.seeding %></td>
        <td><%= contestant.bracketcontest %></td>
        <td><%= contestant.grouping %></td>
        <td><%= contestant.place %></td>
        <td><%= link_to 'Show', contestant %></td>
        <td><%= link_to 'Edit', edit_contestant_path(contestant) %></td>
        <td><%= link_to 'Destroy', contestant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contestant', new_contestant_path %>
