<h1>Dump of Regular Contests</h1>

<br>

<table>
  <thead>
    <tr>
        <th>Date</th>
        <th>Time</th>
        <th>Teams</th>
        <th>Where</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  
    <% @regularcontests.sort{ |a,b| a.updated_at <=> b.updated_at }.reverse.each do |regularcontest| %>
      <tr>
        <td><%= regularcontest.display_date  %></td>
        <td><%= regularcontest.display_time  %></td>
        <td><%= regularcontest.awaycontestant.fullname if regularcontest.awaycontestant %>
        at <%= regularcontest.awaycontestant.opponent.fullname if regularcontest.awaycontestant %></td>
        <td><%= regularcontest.venue_name  %></td>
        <td><%= link_to 'Show', competition_regularcontest_path(@competition, regularcontest)  %></td>
        <td><%= link_to 'Edit', edit_competition_regularcontest_path(@competition, regularcontest) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Regularcontest', new_regularcontest_path %>
