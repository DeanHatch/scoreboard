<h1>Regular Contests</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Venue</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regularcontests.each do |regularcontest| %>
      <tr>
        <td><%= regularcontest.display_date %></td>
        <td><%= regularcontest.display_time %></td>
        <td><%= regularcontest.venue_name %></td>
        <td><%= regularcontest.status %></td>
        <td><%= link_to 'Show', regularcontest %></td>
        <td><%= link_to 'Edit', edit_regularcontest_path(regularcontest) %></td>
        <td><%= link_to 'Destroy', regularcontest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Regularcontest', new_regularcontest_path %>
