
<% content_for :wrapped do %>
To add a new Venue, simply type in it&apos;s name and press ENTER.
<br/>To Change or Remove a Venue, click on the name of the Venue.
<% end %>

<div class="dividedContent">
<div class="leftContent">
<h1>Add a New Venue</h1>

<%= render 'venueform' %>


<h1>Venues</h1>

<table class="schedule">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @venues.sort{ |a,b| a.name <=> b.name }.each do |venue| %>
      <tr>
        <td><%= link_to venue.name, edit_venue_path(venue) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<!-- %= link_to 'New Venue', new_venue_path % -->
</div>
<div class="rightContent">
<h1>Add a New Date</h1>

<%= render 'validdates/dateform' %>



<h1>Dates</h1>

<table class="schedule">
  <thead>
    <tr>
      <th>Date</th>
    </tr>
  </thead>

  <tbody>
    <% @validdates.sort{ |a,b| a.gamedate <=> b.gamedate }.each do |validdate| %>
      <tr>
        <td><%= link_to validdate.gamedate, edit_validdate_path(validdate) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>
</div>
